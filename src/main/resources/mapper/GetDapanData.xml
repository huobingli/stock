<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stock.project.dao.GetDapanData">
    <resultMap id="resultEntityMap" type="com.stock.project.dao.DapanLimitUpDownEntity">
        <id column="id" property="id"  />
        <result column="time" property="time" />
        <result column="limitupcount" property="limitupcount" />
        <result column="limitdowncount" property="limitdowncount" />
        <result column="prelimitup" property="prelimitup" />
    </resultMap>

    <select id="SelectList" resultMap="resultEntityMap">
        select id, time, limitupcount, limitdowncount, prelimitup from dapan order by time asc;
    </select>
</mapper>